#!/usr/bin/env python3
import sys,traceback
from c_Shell import c_Shell
from c_TerminalUserInterface import c_termianlUserInterface

def main():
    uI = c_termianlUserInterface()
    try:
        c_Shell(uI)
    except Exception as e:
        uI.m_end()
        traceback.print_exc()
        sys.exit(1)



if __name__ == "__main__":
    main()
